{
  "name": "hamish-blog",
  "type": "module",
  "version": "1.0.0",
  "description": "Hamish's personal blog built with Astro and Sanity",
  "scripts": {
    "dev": "astro dev --config astro.config.dev.ts",
    "start": "astro dev",
    "build": "astro check && astro build",
    "test": "tsx --test tests/**/*.test.ts",
    "preview": "astro preview",
    "astro": "astro",
    "studio:dev": "npm --prefix studio-production run dev",
    "studio:build": "npm --prefix studio-production run build",
    "convert:webp": "node scripts/convert-png-to-webp.js",
    "update:references": "node scripts/update-references.js",
    "convert:all": "npm run convert:webp && npm run update:references"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.8",
    "@astrojs/netlify": "^6.5.4",
    "@astrojs/react": "^4.4.2",
    "@portabletext/to-html": "^5.0.1",
    "@sanity/astro": "^3.2.11",
    "@sanity/client": "^7.14.1",
    "@sanity/image-url": "^2.0.3",
    "@tailwindcss/vite": "^4.0.1",
    "astro": "^5.1.10",
    "github-slugger": "^2.0.0",
    "js-yaml": "^4.1.0",
    "rehype-format": "^5.0.1",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-directive": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "sanity": "^5.6.0",
    "sharp": "^0.32.6",
    "typescript": "^5.6.3",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/js-yaml": "^4.0.9",
    "tailwindcss": "^4.0.1",
    "tsx": "^4.20.3"
  }
}
